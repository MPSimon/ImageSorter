IMAGESORTER_SECRET_KEY=replace-with-long-random-string
IMAGESORTER_PASSWORD=replace-with-password
# Optional: enables token-based uploads
IMAGESORTER_UPLOAD_TOKEN=
# Upload request limit in MB (20 MB default)
IMAGESORTER_MAX_UPLOAD_MB=20
IMAGESORTER_DATA_ROOT=/data

# Optional (used by deploy/nginx/imagesorter.conf.template when rendering nginx config)
IMAGESORTER_SERVER_NAME=images.bitreq.nl
